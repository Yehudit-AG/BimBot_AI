# Database Configuration
POSTGRES_DB=bimbot_ai_wall
POSTGRES_USER=bimbot_user
POSTGRES_PASSWORD=bimbot_password
DATABASE_URL=postgresql://bimbot_user:bimbot_password@bimbot_postgres:5432/bimbot_ai_wall

# Redis Configuration
REDIS_URL=redis://bimbot_redis:6379/0

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
SECRET_KEY=your-secret-key-here

# File Storage
UPLOAD_DIR=/app/uploads
ARTIFACTS_DIR=/app/artifacts
MAX_FILE_SIZE=100MB

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8000

# Worker Configuration
WORKER_CONCURRENCY=4
JOB_TIMEOUT=3600